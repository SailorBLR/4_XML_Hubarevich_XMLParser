        <voucher-type>holiday_tour</voucher-type>
        <!--Days/nights of staying-->
        <number-days-nights>2/1</number-days-nights>
        <!--Transfer type (bus,train,plain)-->
        <transfer>train</transfer>
        <!--Hotel/apartment/hostel/camping details-->
        <hotel>
            <city>Saint-Petersburg</city>
            <!--type-->
            <type>Hotel</type>
            <!--name-->
            <name>Sport</name>
            <!--hotel ranking (stars)-->
            <stars>3</stars>
            <!--type of food-->
            <food>BB</food>
            <!--tv equipment-->
            <tv-equipe>true</tv-equipe>
            <!--air condition equipment-->
            <air-condition>false</air-condition>
            <!--room capacity-->
            <persons>2</persons>
            <!--elevator equipment-->
            <elevator>floor</elevator>
        </hotel>
        <!--Cost per night in US dollars-->
        <cost>50</cost>
    </international-voucher>


